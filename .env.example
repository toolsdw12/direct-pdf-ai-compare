AZURE_COMPUTER_VISION_KEY=xxxxxx
AZURE_COMPUTER_VISION_ENDPOINT=xxxxxx

# Azure Document Intelligence OCR Configuration
AZURE_DOCUMENT_INTELLIGENCE_KEY=xxxxxx
