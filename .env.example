MISTRAL_API_KEY=xxxxxx

ANTHROPIC_API_KEY=xxxxxx


TELEGRAM_BOT_TOKEN=xxxxxx
TELEGRAM_CHAT_ID=xxxxxx

OPENAI_API_KEY=xxxxxx

