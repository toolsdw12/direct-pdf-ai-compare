AWS_ACCESS_KEY_ID=xxxxxx
AWS_SECRET_ACCESS_KEY=xxxxxx

# Google Cloud Document AI Configuration
GOOGLE_PROJECT_ID=xxxxxx
GOOGLE_LOCATION=us
